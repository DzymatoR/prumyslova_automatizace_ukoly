import pandas as pd
from pathlib import Path

path = Path('DU3') / 'data_temperature.csv'

# načte csv soubor s oddělovačem středník - pomocí knihovny pandas, uloží do proměnné df
# je potřeba správně definovat separátor, jinak se data načtou špatně
df = pd.read_csv(path,sep=";")

# vypíše prvních 5 řádků dataframe
print(df.head())

